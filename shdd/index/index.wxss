.content>view:nth-of-type(1){
    width: 100%;
    height: 356rpx;
    background: #fff;
    font-size: 26rpx;
    margin-bottom: 10rpx;
}
.content>view:nth-of-type(1)>view{
    width: 100%;
    height: 90rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #DEDEDE;
}
.content>view:nth-of-type(1)>view>text{
    display: inline;
    width: 25%;
    height: 100%;
    text-align: center;
    line-height: 90rpx;
}
.content>view:nth-of-type(2){
    width: 100%;
    height: 804rpx;
    padding: 30rpx 20rpx;
    box-sizing:border-box;
    background: #fff;
    margin-bottom: 10rpx;
    
}
